     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float iLight, shine, iSpec, iR, indexF;
uniform vec4 cL;
uniform samplerCube txReflection;

varying vec4 vPos, vMD, vMA, vMS;
varying vec3 vN, vld, vCam;

void main(void)
{

   vec3 r;
   float dotLN;
   vec4 cSpec, cDif, colorfinal;

   //Componente difuso
   dotLN = max( dot( normalize(vld), normalize(vN) )   , 0.0);
   cDif = cL * vMD * dotLN;
   
   //Componente Especular
   cSpec = vec4(0.0,0.0,0.0,0.0);
   r = normalize(reflect(-normalize(vld),vN));
   cSpec = vMS * pow( max( dot(r,-normalize(vPos.xyz)) , 0.0)  ,shine);

   //REFLEXION
   vec3 vReflection = normalize(reflect(-normalize(vCam),vN));
   vec4 envColor = texture(txReflection, vReflection);
   
   //FRESNEL "SIMPLE"
   float Kr = 1.0 - dot(normalize(vCam), vN);
   Kr = pow(Kr, indexF);

   colorfinal = vMA + iLight * (cDif + iSpec*cSpec + iR*envColor*Kr);

   gl_FragColor = colorfinal;
}]]></RmShaderCode></RmGLShader>
