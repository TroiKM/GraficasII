     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float iLight, shine, iSpec, rU, rV;
uniform vec4 cL;
uniform bool useNormalM;

varying vec4 vPos, vMD, vMA, vMS;
varying vec3 vN, vld, vC;
varying vec2 coordUV;

uniform sampler2D texCC01;
uniform sampler2D texSP01, texNP01;

void main(void)
{

   vec3 r;
   float dotLN;
   vec4 cSpec, cDif, colorfinal;

   vec2 auxcoordUV;
   
   auxcoordUV.s = mod(rU*coordUV.s, 1.0);
   auxcoordUV.t = mod(rV*coordUV.t, 1.0);
   
   


   //Textura
   vec4 ccTex01 = texture2D(texCC01, auxcoordUV);
   vec4 spTex01 = texture2D(texSP01, auxcoordUV);
   vec3 npTex01 = (texture2D(texNP01,auxcoordUV).xyz * 2.0) - 1.0;
   
   vec3 N;
   if (useNormalM)
      N = normalize(npTex01);
   else
      N = normalize(vN);
      

   //Componente difuso
   dotLN = max( dot( normalize(vld), normalize(N) )   , 0.0);
   cDif = cL * vMD * ccTex01 * dotLN ;
   
   //Componente Especular
   cSpec = vec4(0.0,0.0,0.0,0.0);
   if (dotLN > 0.0) {
      r = normalize(reflect(-normalize(vld),N));
      cSpec = vMS * pow( max( dot(r,normalize(vC)) , 0.0)  ,shine) * spTex01 ;
   }

   colorfinal = vMA + iLight * (cDif + iSpec*cSpec);

   gl_FragColor = colorfinal;
}]]></RmShaderCode></RmGLShader>
