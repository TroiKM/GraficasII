     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform vec4 lightDirection;

varying vec3 n;
varying vec4 ld;

void main(void)
{

   n = gl_NormalMatrix * gl_Normal;
   
   ld = lightDirection;


   gl_Position = ftransform();
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform vec4 c1;
uniform vec4 c2;
uniform vec4 c3;
uniform vec4 c4;

uniform float intensidadL;

uniform float f1;
uniform float f2;
uniform float f3;

varying vec3 n;
varying vec4 ld;


void main(void)
{

   float cosV;
   vec4 color = vec4(0.0,0.0,0.0,0.0);
   
   cosV = dot(vec3(normalize(ld)),normalize(n));


   if (cosV > f1)
      color = c1;
   else if (cosV > f2)
      color = c2;
   else if (cosV > f3)
      color = c3;
   else
      color = c4;
      
      

   gl_FragColor = intensidadL*color;
}]]></RmShaderCode></RmGLShader>
