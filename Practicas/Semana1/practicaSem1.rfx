     <RmGLShader NAME="Vertex Program" API="OpenGL" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform vec4 lightVector, cMD, cMA, cMS;
uniform bool tLight;

varying vec4 vPos, vMD, vMA, vMS;
varying vec3 vN, vld;

varying vec2 texcoord;

void main(void)
{

   vec4 ld;

   vN = gl_NormalMatrix * gl_Normal;
   ld = gl_ModelViewMatrix * lightVector;
   
   texcoord = gl_MultiTexCoord0.st;
   
   //Para la luz puntual
   vPos = gl_ModelViewMatrix * gl_Vertex;

   ld.w = float(tLight);


   if (int(ld.w) == 1)
      vld = ld.xyz - vPos.xyz;
   else
      vld = ld.xyz;


   vMD = cMD;
   vMA = cMA;
   vMS = cMS;
   
   gl_Position = ftransform();
}]]></RmShaderCode></RmGLShader>
     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
uniform float iLight, shine, iSpec, repeatU, repeatV;
uniform vec4 cL;


varying vec4 vPos, vMD, vMA, vMS;
varying vec3 vN, vld;
varying vec2 texcoord;

void main(void)
{

   vec3 r;
   float dotLN;
   vec4 cSpec, cDif, colorfinal;
   
   vec2 ftexcoord;
   ftexcoord.s = mod(repeatU*texcoord.s,1.0);
   ftexcoord.t = mod(repeatV*texcoord.t,1.0);

   float A, B;
   A = 1.0 - step(0.5, ftexcoord.s);
   B = step(0.5,ftexcoord.s);
   
   float ctp = (1.0-step(0.5,ftexcoord.t))*A + (step(0.5,ftexcoord.t))*B;;
   
   vec4 cColorTP = vec4(ctp,ctp,ctp,1.0);
   

   //Componente difuso
   dotLN = max( dot( normalize(vld), normalize(vN) )   , 0.0);
   cDif = cL * vMD * dotLN * cColorTP;
   
   //Componente Especular
   cSpec = vec4(0.0,0.0,0.0,0.0);
   r = normalize(reflect(-normalize(vld),vN));
   cSpec = vMS * pow( max( dot(r,-normalize(vPos.xyz)) , 0.0)  ,shine);


   colorfinal = vMA + iLight * (cDif + iSpec*cSpec);

   gl_FragColor = colorfinal;
}]]></RmShaderCode></RmGLShader>
